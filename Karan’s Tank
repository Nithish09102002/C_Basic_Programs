#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    float a,b,c,d;
    float e,f;
    cin>>a>>b>>c>>d;
    e=(3.14*(a*a)*b);
    f=(c*d);
    cout<<fixed<<setprecision(1);
    if (e<f)
    {
        cout<<"The tank can be filled within "<<d<<" hours";
    }
    else
    {
        cout<<"The tank cannot be filled within "<<d<<" hours";
    }
}
